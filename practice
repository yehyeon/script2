    <script>
    var shape = ["♡","◇","♠","♣"];
    var deck = [];
    var user = [];

    function start(){

        for(var i=0 in shape){
            j = 0;
            while(j < 13){

                deck.push(shape[i]+j+1)
                j++
            }
        }
        console.log(deck)

    }

</script>




</head>
<body>
    <button onclick="start()">시작</button>
    <button onclick="">콜</button>
